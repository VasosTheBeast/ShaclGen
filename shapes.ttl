@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://www.example.org#Dog_Shape> a sh:NodeShape ;
    sh:targetClass <http://www.example.org#Dog> .

<http://www.example.org#Person_Shape> a sh:NodeShape ;
    sh:property <http://www.example.org#hasAge_PropertyShape>,
        <http://www.example.org#hasFriend_PropertyShape>,
        <http://www.example.org#hasName_PropertyShape> ;
    sh:targetClass <http://www.example.org#Person> .

<http://www.example.org#Student_Shape> a sh:NodeShape ;
    sh:targetClass <http://www.example.org#Student> .

<http://www.example.org#hasAge_PropertyShape> a sh:PropertyShape ;
    sh:datatype xsd:integer ;
    sh:maxCount 1 ;
    sh:path <http://www.example.org#hasAge> .

<http://www.example.org#hasFriend_PropertyShape> a sh:PropertyShape ;
    sh:class <http://www.example.org#Person> ;
    sh:path <http://www.example.org#hasFriend> .

<http://www.example.org#hasName_PropertyShape> a sh:PropertyShape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path <http://www.example.org#hasName> .

